  "gamesScripts": [
    {
      "script": {
        "allGames": false,
        "global": false,
        "scriptEngine": "Powershell",
        "operatingSystem": [
          "Windows"
        ],
        "scriptEvents": [
          "CustomServiceAction",
          "CustomServiceIcon"
        ],
        "name": "Backup Config Files",
        "description": "Backup your config files",
        "code": "Write-Host $(Get-ExecutionPolicy -Verbose)\r\nImport-Module Microsoft.PowerShell.Archive -Verbose\r\n# \u0026 \u0022C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\Modules\\Microsoft.PowerShell.Archive\\Microsoft.PowerShell.Archive.psm1\u0022\r\n\r\n# Define the source folder and backup directory\r\n$sourceFolder = Join-Path $ThisService.RootDirectory \u0022garrysmod\\cfg\u0022\r\n$backupDirectory = Join-Path $ThisService.RootDirectory \u0022Backups\u0022\r\n\r\n# Ensure the backup directory exists\r\nif (!(Test-Path $backupDirectory)) {\r\n    New-Item -ItemType Directory -Path $backupDirectory | Out-Null\r\n}\r\n\r\n# Generate the timestamped zip file name\r\n$timestamp = (Get-Date).ToString(\u0022yyyyMMdd-HHmmss\u0022)\r\n$backupFileName = \u0022$timestamp.zip\u0022\r\n$backupFilePath = Join-Path $backupDirectory $backupFileName\r\n\r\nWrite-Host \u0022Creating backup: $backupFileName\u0022\r\n\r\n# Create the zip archive\r\ntry {\r\n    Compress-Archive -Path $sourceFolder -DestinationPath $backupFilePath -Force -Verbose\r\n    Write-Host \u0022Backup created successfully: $backupFileName\u0022\r\n} catch {\r\n    Write-Host \u0022Error creating backup: $_\u0022 -ForegroundColor Red\r\n}",
        "runImpersonated": false,
        "stopService": false,
